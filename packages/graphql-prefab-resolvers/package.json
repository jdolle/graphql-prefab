{
  "name": "graphql-prefab-resolvers",
  "description": "Reduce resolver boilerplate by configuring resolvers through JSON",
  "version": "0.0.2",
  "main": "dist/index.js",
  "keywords": [
    "graphql",
    "resolver",
    "apollo",
    "json",
    "config",
    "gql"
  ],
  "repository": {
    "type": "git",
    "url": "github.com/jdolle/graphql-prefab"
  },
  "author": "Jeff Dolle (jdolle@gmail.com)",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/* && tsc",
    "demo": "BACKEND_URL=https://backend.staging.acorns.io ts-node-dev --respawn --type-check ./examples/demo-server/index.ts",
    "lint": "tslint --project tsconfig.json --format stylish \"src/**\"",
    "test": "jest --forceExit --notify",
    "test:ci": "jest --ci --no-watchman"
  },
  "devDependencies": {
    "@types/humps": "^1.1.2",
    "@types/jest": "^22.2.2",
    "@types/moxios": "^0.4.8",
    "@types/node": "^9.6.0",
    "@types/pg": "^7.4.5",
    "@types/ramda": "^0.25.20",
    "graphql-import": "^0.5.0",
    "graphql-yoga": "^1.6.1",
    "jest": "^22.4.3",
    "moxios": "^0.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "ts-node-dev": "^1.0.0-pre.18",
    "tslint": "^5.9.1",
    "tslint-consistent-codestyle": "^1.12.2",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "graphql-resolvers": "^0.2.2",
    "humps": "^2.0.1",
    "pg": "^7.4.1",
    "ramda": "^0.25.0",
    "tslib": "^1.9.0",
    "velocityjs": "^1.0.1"
  },
  "jest": {
    "cacheDirectory": "jestCache",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
