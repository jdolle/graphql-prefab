{
  "typeName": "Query",
  "fields": {
    "acceptanceDocument": [
      {
        "use": "http",
        "options": {
          "url": "$env.BACKEND_URL/v1/acceptance_documents/$args.type",
          "timeout": 5000
        }
      },
      {
        "use": "ramda",
        "options": {
          "fn": "path",
          "args": [
            ["acceptance_document"]
          ]
        }
      },
      { "use": "camelizeKeys" }
    ]
  }
}
